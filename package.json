{
  "name": "starter-typescript-library",
  "version": "0.1.0",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/starter-typescript-library.cjs.js",
  "module": "dist/starter-typescript-library.esm.js",
  "typings": "dist/types/starter-typescript-library.d.ts",
  "scripts": {
    "prebuild": "del-cli dist",
    "build": "rollup -c rollup.config.js",
    "start": "rollup -c rollup.config.js -w",
    "test": "jest"
  },
  "browser": "dist/starter-typescript-library.js",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "23.3.5",
    "@types/node": "10.11.7",
    "del-cli": "1.1.0",
    "jest": "23.6.0",
    "rollup": "0.66.6",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-livereload": "0.6.0",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-serve": "0.6.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "3.0.0",
    "rollup-plugin-typescript2": "0.17.1",
    "ts-jest": "23.10.4",
    "typedoc": "0.13.0",
    "typescript": "3.1.3"
  }
}
